{
    "manifest_version": 3,
    "name": "Feed filter",
    "author": "Manvith",
    "version": "1.0.0",
    "description": "üõ°Ô∏è AI-powered  extension that blocks cringe posts on linkedin.",
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "https://www.linkedin.com/feed/*"
            ]
        }
    ],
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "action": {
        "default_popup": "pages/popup.html",
        "default_icon": {
            "16": "images/icon.png",
            "32": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        },
        "default_title": "LinkedIn Blocker",
        "default_width": 500,
        "default_height": 700
    },
    "options_page": "./pages/settings.html",
    "permissions": [
        "storage"
    ]
}